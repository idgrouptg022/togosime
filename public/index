var link_updated = 0;

    var groupCache = {};
    function getElementsById(id)
    {
        if (!groupCache[id])
            groupCache[id] = [];
    
        var nodes = groupCache[id];
        for (var x=0; x<nodes .length; x++)
        {
            if (nodes[x].id != "")
            {
                nodes.splice(x, 1);
                x--;
            }
        }
        
        var tmpNode = document.getElementById(id);
        while(tmpNode)
        {
            nodes.push(tmpNode);
            tmpNode.id = "";
            tmpNode = document.getElementById(id);
        }
        
        return nodes;
    }

    function page_loaded()
    {
        if (link_updated == 0) 
        {
            //e = document.getElementById('spn_download_link');
            els = getElementsById('spn_download_link');
            for(var i in els)
            {
                e = els[i];
                if (e)
                    e.innerHTML = link_dec;
            }
            link_updated = 1;
        }
    }
    window.onload = page_loaded;
    setInterval("page_loaded()", 10000); /* "incase sh*t" timer */

